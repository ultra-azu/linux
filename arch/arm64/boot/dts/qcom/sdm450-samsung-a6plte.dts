*/
};

&dsi0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	vdd-supply = <&pm8953_l17>;
	vdda-supply = <&pm8953_s3>;
	vddio-supply = <&pm8953_l6>;

	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&pmx_mdss_default &mdss_te_default>;
	pinctrl-1 = <&pmx_mdss_sleep &mdss_te_default>;

	panel@0 {
		compatible = "samsung,s6e3fa7-ams604nl01-fhd";
		reg = <0>;

		reset-gpios = <&msmgpio 50 GPIO_ACTIVE_HIGH>;

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				panel_in: endpoint {
					remote-endpoint = <&dsi0_out>;
				};
			};
		};
	};

	ports {
		port@1 {
			endpoint {
				remote-endpoint = <&panel_in>;
				data-lanes = <0 1 2 3>;
			};
		};
	};
};

&spmi_regulators {
	status = "okay";
};

&dsi_phy0 {
	vddio-supply = <&pm8953_l6>;
	vcca-supply = <&pm8953_l3>;
};

&i2c_2 {
	status = "okay";

	touchscreen@48 {
		compatible = "samsung,s6sy761";

		reg = <0x48>;
		interrupt-parent = <&msmgpio>;
		interrupts = <59 IRQ_TYPE_EDGE_FALLING>;

		touchscreen-size-x = <2048>;
		touchscreen-size-y = <2048>;

		avdd-supply = <&reg_vdd_tsp>;
		vdd-supply = <&reg_vdd_tsp>;

		pinctrl-names = "default";
		pinctrl-0 = <&ts_int_default>;
	};
};

&msmgpio {
	sm5708_i2c_default: sm5708_muic_default {
		pinmux {
			pins = "gpio98", "gpio99";
			function = "gpio";
		};
		pinconf {
			pins = "gpio98", "gpio99";
			drive-strength = <2>;
			bias-disable;
		};
	};
	sm5708_interrupt_default: sm5708_interrupt_default {
		pinmux {
			pins = "gpio63";
			function = "gpio";
		};
		pinconf {
			pins = "gpio63";
			drive-strength = <2>;
			bias-disable;
		};
	};
	sm5708_rst_default: sm5708_rst_default {
		pinmux {
			pins = "gpio61";
			function = "gpio";
		};
		pinconf {
			pins = "gpio61";
			drive-strength = <2>;
			bias-disable;
			input-enable;
		};
	};
	charger_int_active: charger_int_active {
		pinmux {
			pins = "gpio60";
			function = "gpio";
		};
		pinconf {
			pins = "gpio60";
			drive-strength = <2>;
			bias-disable;
		};
	};
	chg_en_default: chg_en_default {
		pinmux {
			pins = "gpio34";
			function = "gpio";
		};
		pinconf {
			pins = "gpio34";
			drive-strength = <2>;
			bias-pull-down;
			output-high;
		};
	};
	ts_int_default: ts_int_default {
		pinmux {
			pins = "gpio59";
			function = "gpio";
		};

		pinconf {
			pins = "gpio59";
			drive-strength = <2>;
			bias-disable;
		};
	};
	tsp_en_default: tsp_en_default {
		pinmux {
			function = "gpio";
			pins = "gpio139";
		};
		pinconf {
			pins = "gpio139";
			drive-strength = <2>;
			bias-disable;
		};
	};
};

&sdhc_1 {
	status = "okay";

	vmmc-supply = <&pm8953_l8>;
	vqmmc-supply = <&pm8953_l5>;

	mmc-hs200-1_8v;
	mmc-hs400-1_8v;

	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&sdc1_clk_on &sdc1_cmd_on &sdc1_data_on  &sdc1_rclk_on>;
	pinctrl-1 = <&sdc1_clk_off &sdc1_cmd_off &sdc1_data_off &sdc1_rclk_off>;
};

&sdhc_2 {
	status = "okay";

	vmmc-supply = <&pm8953_l11>;
	vqmmc-supply = <&pm8953_l12>;

	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&sdc2_clk_on &sdc2_cmd_on &sdc2_data_on &sdc2_cd_on>;
	pinctrl-1 = <&sdc2_clk_off &sdc2_cmd_off &sdc2_data_off &sdc2_cd_off>;

	cd-gpios = <&msmgpio 133 GPIO_ACTIVE_LOW>;
};

&msmgpio {
	gpio-reserved-ranges = <12 4>; // Fingerprint sensor

	gpio_hall_sensor_default: gpio-hall-sensor-default {
		mux {
			pins = "gpio44";
			function = "gpio";
		};
		config {
			pins = "gpio44";
			drive-strength = <2>;   /* 16 mA */
			bias-disable;
			input-enable;
		};
	};
	pmx_mdss_default: pmx-mdss-default {
		mux { /* Reset and power */
			pins = "gpio50", "gpio35", "gpio93";
			function = "gpio";
		};

		config {
			pins = "gpio50", "gpio35", "gpio93";
			drive-strength = <8>; /* 8 mA */
			bias-disable = <0>; /* no pull */
			output-high;
		};
	};

	pmx_mdss_sleep: pmx-mdss-sleep {
		mux {
			pins = "gpio50", "gpio35", "gpio93";
			function = "gpio";
		};

		config {
			pins = "gpio50", "gpio35", "gpio93";
			drive-strength = <2>; /* 2 mA */
			bias-pull-down; /* pull down */
		};
	};
};

&gpio_key_default {
	mux {
		pins = "gpio85";
		function = "gpio";
	};

	config {
		pins = "gpio85";
		drive-strength = <2>;
		bias-pull-up;
	};
};

&usb3 {
	status = "okay";
	//extcon = <&muic>, <&muic>;
};

&usb3_dwc3 {
	dr_mode = "peripheral";
	//maximum-speed = "high-speed";
	//extcon = <&muic>, <&muic>;
	
	status = "okay";
};

&hsusb_phy {
	phy-supply = <&pm8953_l3>;
	vdda-pll-supply = <&pm8953_l7>;
	vdda-phy-dpdm-supply = <&pm8953_l13>;

	status = "okay";
};

&ssusb_phy {
	vdda-phy-supply = <&pm8953_l3>;
	vdda-pll-supply = <&pm8953_l7>;

	status = "okay";
};

&smd_rpm_regulators {
	compatible = "qcom,rpm-pm8953-regulators";

	s1 { // Modem
		regulator-min-microvolt = <870000>;
		regulator-max-microvolt = <1156000>;
	};
	s3 { // CSI
		regulator-min-microvolt = <1225000>;
		regulator-max-microvolt = <1225000>;
	};
	s4 { // Audio codec
		regulator-min-microvolt = <1900000>;
		regulator-max-microvolt = <2050000>;
	};
	l1 { // unused
		regulator-min-microvolt = <1000000>;
		regulator-max-microvolt = <1100000>;
	};
	l2 { // unused
		regulator-min-microvolt = <975000>;
		regulator-max-microvolt = <1225000>;
	};
	l3 { // Usb, mdss phy
		regulator-min-microvolt = <925000>;
		regulator-max-microvolt = <925000>;
		regulator-allow-set-load;
	};
	l5 { // expander, wcndss, sdhci, codec
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
	};
	l6 { // DSI
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
	};
	l7 { // wcnss, modem, usb phy
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1900000>;
	};
	l8 { // sdhci, .
		regulator-min-microvolt = <2900000>;
		regulator-max-microvolt = <2900000>;
	};
	l9 { // wcnss
		regulator-min-microvolt = <3000000>;
		regulator-max-microvolt = <3300000>;
	};
	l10 { // unused
		regulator-min-microvolt = <2850000>;
		regulator-max-microvolt = <2850000>;
	};
	l11 { // sdhci, accel sensor
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <3000000>;
	};
	l12 { // sdhci,
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3000000>;
	};
	l13 { // audio codec, usb phy
		regulator-min-microvolt = <3075000>;
		regulator-max-microvolt = <3125000>;
	};
	l15 { // unused
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
	};
	l16 { // unused
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
	};
	l17 { // dsi
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <3000000>;
	};
	l19 { // wcnss
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1350000>;
	};
	l22 { // unused
		regulator-min-microvolt = <2800000>;
		regulator-max-microvolt = <2850000>;
	};
	l23 { // camera
		regulator-min-microvolt = <975000>;
		regulator-max-microvolt = <1225000>;
	};
};
